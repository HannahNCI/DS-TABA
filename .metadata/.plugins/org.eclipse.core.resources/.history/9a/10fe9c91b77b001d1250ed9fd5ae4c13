package javax.jmdns;

import java.net.InetAddress;

import GUI.GUI.MyServiceListener;

public interface JmDNS {

	@Override
	int hashCode();
	@Override
	boolean equals(Object obj);
	Object clone() throws CloneNotSupportedException;
	@Override
	String toString();
	
	void close();

	void registerService(ServiceInfo serviceInfo);

	void addServiceListener(String service_type, GUI.GUI.MyServiceListener myServiceListener);

}
